<Window x:Class="WPFView.CadastroEstabelecimento"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFView"
        mc:Ignorable="d"
        Title="CadastroEstabelecimento" Height="450" Width="800">
    <Grid>
        <Label Content="Dados do Estabelecimento:" HorizontalAlignment="Left" Margin="23,22,0,0" VerticalAlignment="Top"/>
        <Label Content="CNPJ:" HorizontalAlignment="Left" Margin="70,53,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="cnpj" HorizontalAlignment="Left" Height="23" Margin="114,56,0,0" TextWrapping="Wrap" Text="Digite o CNPJ" VerticalAlignment="Top" Width="120"/>
        <Label Content="Nome Fantasia:" HorizontalAlignment="Left" Margin="70,96,0,0" VerticalAlignment="Top" RenderTransformOrigin="-1.114,0.526"/>
        <TextBox x:Name="nomeFantasia" HorizontalAlignment="Left" Height="23" Margin="166,99,0,0" TextWrapping="Wrap" Text="Digite o nome fantasia" VerticalAlignment="Top" Width="333"/>
        <Label Content="Categoria:" HorizontalAlignment="Left" Margin="70,140,0,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="139,144,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Alimentação"/>
        </ComboBox>

    </Grid>
</Window>
